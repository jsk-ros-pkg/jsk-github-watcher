<% include header %>
<div class="container">
  <div class="page-header">
    <h1><%= title %></h1>
  </div>
</div>
<% if (done) { %>
<div class="alert alert-success" role="alert">done <%= done_count %> repositories</div>
<% } %>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <p class="lead">Welcome to <%= title %></p>
    </div>
  </div>
  <% var other_orgs = ["jsk-ros-pkg", "euslisp", "tork-a", "rtm-ros-robotics",
     "ros", "ros-planning", "ros-perception", "ros-visualization", "ros-infrastructure", "ros-drivers",
     "PR2", "pr2-debs", "willowgarage", "wg-perception", "mitdrc", "code-iai", "robotics-in-concert", "OctoMap", "davetcoleman"]; %>
  <% for (var i = 0; i < other_orgs.length; i++) { %>
  <div class="button-row">
    <div class="row">
      <div class="col-md-4 col-md-offset-4">
        <a href="/watch/<%= other_orgs[i]%>">
          <button type="button" class="btn btn-lg btn-block">
            watch <%= other_orgs[i]%>!
          </button>
        </a>
      </div>
    </div>
  </div>
  <% } %>
</div>
<% include footer %>
